{
  "name": "AdventistEducation",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "start-web": "NODE_ENV=development PORT=3000 react-scripts start",
    "start-web-prod": "NODE_ENV=production PORT=3000 react-scripts start",
    "build-web": "react-scripts build",
    "start-via-script": "node scripts/start.js",
    "build-via-script": "node scripts/build.js",
    "test-web": "react-scripts test --env=jsdom",
    "eject-web": "react-scripts eject",
    "electron": "NODE_ENV=development electron .",
    "electron-prod": "NODE_ENV=production electron .",
    "react-devtools": "react-devtools",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-webdesk": "npm-run-all -p watch-css start-web",
    "start-webdesk-via-script": "npm-run-all -p watch-css start-via-script",
    "build-webdesk": "npm run build-css && react-scripts build",
    "build-package-mac": "NODE_ENV=production node scripts/build.js && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "build-package-win": "NODE_ENV=production node scripts/build.js && electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=opetstudio --version-string.FileDescription=CE --version-string.ProductName=\"AdventistEducation\"",
    "build-package-linux": "NODE_ENV=production node scripts/build.js && electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-mac": "NODE_ENV=production electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=opetstudio --version-string.FileDescription=CE --version-string.ProductName=\"AdventistEducation\"",
    "package-linux": "NODE_ENV=production electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "@shoutem/ui": "^0.21.3",
    "app-root-path": "^2.0.1",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "base-64": "^0.1.0",
    "bootstrap": "^3.3.7",
    "browser-window": "^0.4.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "crypto": "^1.0.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "electron": "~1.6.2",
    "electronify-server": "^0.6.2",
    "eslint": "4.4.1",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "firebase": "^4.3.0",
    "fs": "0.0.1-security",
    "fs-extra": "3.0.1",
    "history": "^4.7.1",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.4",
    "json2csv": "^3.11.2",
    "lodash": "^4.17.19",
    "menu": "^0.2.5",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "node-pre-gyp": "^0.6.37",
    "node-sass-chokidar": "0.0.3",
    "node-uuid": "^1.4.8",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "os": "^0.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dev-utils": "^3.1.0",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.10",
    "react-hot-loader": "^1.3.1",
    "react-native": "0.48.1",
    "react-native-contacts": "^0.9.0",
    "react-native-device-info": "^0.11.0",
    "react-native-fcm": "^9.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "^4.0.0-beta.18",
    "react-native-tab-view": "0.0.69",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "realm": "^1.12.0",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "xlsx": "^0.11.3",
    "xlsx-style": "^0.8.13"
  },
  "devDependencies": {
    "babel-jest": "21.0.0",
    "babel-preset-react-native": "3.0.2",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^9.0.1",
    "jest": "21.0.0",
    "react-devtools": "^2.5.0",
    "react-scripts": "1.0.13",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "main": "electron-starter.js",
  "homepage": ".",
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "opetstudio",
  "license": "MIT"
}
